	#revolut-ad
		background-color $background-2 !important
		.button
			background $background-1 !important
			border $border-style !important
			color $text-2 !important
			&:hover
				background-color $highlight !important
		.text
			color $text-2 !important
			> a
				color $theme !important
		.title > a
			color $text-1 !important
		> .shadow
			background -moz-linear-gradient(top, transparent 0, transparent 10%, $background-1 100%) !important
			background -webkit-gradient(linear, left top, left bottom, color-stop(0, transparent), color-stop(45%, transparent), color-stop(100%, $background-1)) !important
			background -webkit-linear-gradient(top, transparent 0, transparent 10%, $background-1 100%) !important
			background -o-linear-gradient(top, transparent 0, transparent 10%, $background-1 100%) !important
			background -ms-linear-gradient(top, transparent 0, transparent 10%, $background-1 100%) !important
			background linear-gradient(to bottom, transparent 0, transparent 10%, $background-1 100%) !important
	#single.content_holder
		background-color $background-2 !important
	#subscribeForm > fieldset > button
		background $background-1 !important
		background-color $background-1 !important
		color $text-2 !important
		&:hover
			background $highlight !important
			background-color $highlight !important
			color $text-1 !important
	#threadList-1
		> table > tbody.body.threadsBody > tr > td.cell.title.padding-small-left > div > div > a
			color $text-1 !important
		.table .head .cell
			color $text-1 !important
			border-top $border-style !important
			border-bottom $border-style !important
			&.select
				border-right $border-style !important
			&.status
				border-left $border-style !important
		tr.hasUnreadPosts td.status
			box-shadow inset 5px 0 0 0 $highlight !important
	#wdgtSideRecentClassifieds > div
		&:nth-child(2),
		&:nth-child(3),
		&:nth-child(4),
		&:nth-child(5)
			> h6
				color $text-1 !important
	.activePolls a
		color $theme !important
		&:hover
			text-decoration $hover-text !important
			opacity $hover-opacity !important
	.ad
		&.adPanorama,
		&.adModule,
		&.adInsider,
		&.adOutsider
			iframe
				border-radius $border-radius
				background-color $background-3
				box-shadow $box-shadow
		&.adModule
			border-bottom none !important
	.articleActions .action.proofArticle
		.actionIcon
			filter $theme-filter !important
		.actionLabel
			color $theme !important
			&:hover
				text-decoration $hover-text !important
	.articleBBCode
		.bbBlockQuote&:after
			color $theme
		.bbExpander
			.expanderFooter
				color $text-2 !important
				&:hover
					text-decoration $hover-text !important
					opacity $hover-opacity !important
			.expanderHeader
				color $theme !important
				&:hover
					text-decoration $hover-text !important
					opacity $hover-opacity !important
			> .expanderFrame > .expanderFade
				background -moz-linear-gradient(top, transparent 0, transparent 10%, $background-1 100%) !important
				background -webkit-gradient(linear, left top, left bottom, color-stop(0, transparent), color-stop(45%, transparent), color-stop(100%, $background-1)) !important
				background -webkit-linear-gradient(top, transparent 0, transparent 10%, $background-1 100%) !important
				background -o-linear-gradient(top, transparent 0, transparent 10%, $background-1 100%) !important
				background -ms-linear-gradient(top, transparent 0, transparent 10%, $background-1 100%) !important
				background linear-gradient(to bottom, transparent 0, transparent 10%, $background-1 100%) !important
		.bbPoll
			.optionBar > div
				background $theme !important
				border none !important
			.pollOptions
				.votesAbs
					color $text-2 !important
				.votesRel
					color $text-2 !important
			.pollQuestion
				color $text-1 !important
				border-bottom $border-style !important
			.pollSums
				.pollMessage
					color $text-2 !important
				.totalVotes
					color $text-1 !important
					border-top $border-style !important
			> .inner
				background $background-1 !important
				background-color $background-1 !important
				border $border-style !important
				color $text-2 !important
		.bbPrisjakt a
			&.prices
				color $text-2 !important
			&:hover.prices
				text-decoration $hover-text !important
				opacity $hover-opacity !important
		.bbRelatedBox
			background-color $background-2 !important
			border none !important
			box-shadow $box-shadow !important
			padding-bottom 1px
		.bbSlideshow .slideFooter
			color $text-2 !important
		.bbTable
			border-top $border-style !important
			border-left $border-style !important
			background-color $background-1 !important
			thead,
			tbody
				th
					background-color $background-3 !important
					color $theme !important
			thead
				tr
					&:first-child th
						border-right 1px solid lighten($border, 5%) !important
						border-bottom 1px solid darken($border, 50%) !important
						&:first-child
							border-right 1px solid lighten($border, 5%) !important
							border-bottom 1px solid lighten($border, 5%) !important
						&:last-child
							border-right 1px solid $border !important
				th
					background-color $background-3 !important
			tbody
				tr
					th
						border-right 1px solid darken($border, 50%) !important
						border-bottom 1px solid lighten($border, 5%) !important
					&:last-child th
						border-bottom 1px solid $border !important
					td
						border-right 1px solid $border !important
						border-bottom 1px solid $border !important
			a
				color $theme !important
				&:hover
					text-decoration $hover-text !important
					opacity $hover-opacity !important
		.bbcode
			.bbParagraph
				color $text-2 !important
			a
				color $theme !important
				&:hover
					text-decoration $hover-text !important
					opacity $hover-opacity !important
		.footerPanel .bbRelatedBox
			border none !important
			box-shadow none !important
	.articleByline .author span
		color $text-2 !important
	.articleCommentsWidget
		background $background-2 !important
		background-color $background-2 !important
		color $text-2 !important
		box-shadow $box-shadow !important
		.boxAction
			.actionLabel
				color $text-1 !important
			a:hover
				.actionIcon,
				.actionLabel
					text-decoration $hover-text !important
					opacity $hover-opacity !important
		.mwHead p
			color $text-2 !important
		.mwItem
			.mwLabelIcon
				filter $theme-filter !important
			a:hover
				.mwIcon,
				.mwLabelDate,
				.mwLabelText
					text-decoration $hover-text !important
					opacity $hover-opacity !important
		.mwLabel
			> .mwLabelDate
				color $text-1 !important
			> .mwLabelText
				color $text-2 !important
		h5 > a
			color $theme !important
			text-decoration none !important
			&:hover
				text-decoration $hover-text !important
				opacity $hover-opacity !important
	.articleContent
		.articleNavi
			.head h5
				color $text-1 !important
			.pageButton
				color $text-1 !important
