/* ==UserStyle==
@name			SweClockers Dark
@namespace		https://github.com/Soitora
@version		3.0.0-beta.13
@description	A modern dark theme for SweClockers
@author			Soitora <simon.mattila@protonmail.com> (https://soitora.com)
@homepageURL	https://github.com/Soitora/SweClockers-Dark
@supportURL		https://github.com/Soitora/SweClockers-Dark/issues
@updateURL		https://raw.githubusercontent.com/Soitora/SweClockers-Dark/master/sweclockers-dark.user.styl
@license		MPL-2.0
@preprocessor	stylus

@var	select		var-ad-height "Banner ad height" {
	"Locked": "locked-banner-ad-height",
	"Default": "default-banner-ad-height"
}
@var	select		var-ad-banner-spacer "Banner ad spacer" {
	"10px": "locked-ad-banner-spacer",
	"Default": "default-ad-banner-spacer"
}
@var	select		var-navigation-style "Navigation style" {
	"Custom": "custom-navigation",
	"Default": "default-navigation"
}
@var	select		var-search-style "Search style" {
	"Custom": "custom-search",
	"Default": "default-search"
}
@var	select		var-scrollbar-style "Scrollbar style" {
	"Custom": "custom-scrollbar",
	"Default": "default-scrollbar"
}
@var	number		var-image-opacity "Image opacity" [0.75, 0, 1, 0.05]
@var	color		var-background-1 "Background color 1" #121212
@var	color		var-background-2 "Background color 2" #1D1D1F
@var	color		var-text-1 "Text color 1" #C8C8C8
@var	color		var-text-2 "Text color 2" #AAAAAA
@var	color		var-border "Border color" #08080B
@var	color		var-border-light "Border color light" #3B3B3B
@var	color		var-border-menus "Border menu color" #000000
@var	color		var-forum-header "Forum header color" #232323
@var	color		var-highlight "Highlight color" #2A2A2D
@var	color		var-warning "Warning color" #EE4757
@var	color		var-theme "Theme color" #F19A57
@var	text		var-warning-filter "Warning filter" brightness(0) saturate(100%) invert(43%) sepia(88%) saturate(1483%) hue-rotate(326deg) brightness(89%) contrast(113%)
@var	text		var-theme-filter "Theme filter" brightness(0) saturate(100%) invert(82%) sepia(26%) saturate(3168%) hue-rotate(320deg) brightness(99%) contrast(91%)
==/UserStyle== */
@-moz-document domain("sweclockers.com")
	/* USERCSS VARIABLES */
	var-background-1 ?= #121212
	var-background-2 ?= #1D1D1F
	var-text-1 ?= #C8C8C8
	var-text-2 ?= #AAAAAA
	var-border ?= #08080B
	var-border-light ?= #3B3B3B
	var-border-menus ?= #000000
	var-forum-header ?= #232323
	var-highlight ?= #2A2A2D
	var-warning ?= #EE4757
	var-theme ?= #F19A57
	var-warning-filter ?= brightness(0) saturate(100%) invert(43%) sepia(88%) saturate(1483%) hue-rotate(326deg) brightness(89%) contrast(113%)
	var-theme-filter ?= brightness(0) saturate(100%) invert(82%) sepia(26%) saturate(3168%) hue-rotate(320deg) brightness(99%) contrast(91%)
	/* GLOBAL VARIABLES */
	var-on-background-1 = var-background-2
	var-on-background-2 = darken(var-background-2, 25%)
	var-on-background-2-button = lighten(var-background-2, 3%)
	var-border-style = 1px solid var-border
	var-border-style-light = 1px solid var-border-light
	var-border-radius = 0.6rem
	var-border-radius-button = 4px
	var-hover-opacity = 0.8
	var-hover-text = none
	var-box-shadow = 0 0 0 1px var-border
	var-box-shadow-inset = inset 0 0 0 1px var-border
	var-text-shadow = 1px 1px 2px var-black
	var-grey-filter = sepia(0) saturate(0.29) brightness(67%) contrast(184%) hue-rotate(179deg) invert(8%)
	var-light-filter = brightness(0) saturate(100%) invert(92%) sepia(0) saturate(489%) hue-rotate(229deg) brightness(90%) contrast(86%)
	var-blue-filter = brightness(0) saturate(100%) invert(41%) sepia(50%) saturate(563%) hue-rotate(159deg) brightness(94%) contrast(89%)
	/* COLOR VARIABLES */
	var-black = #000000
	var-white = #ffffff
	var-beige = #9c6500
	var-blue = #305d84
	var-green = #1c5a00
	var-pink = #b34d84
	/* COLOR VARIABLES - EVENTS */
	var-optionPaskrea = #b59600
	var-optionCes = #00a0dc
	var-optionComputex = #ad0060
	var-optionDreamhack = #5e2f8e
	var-optionVr = #8111e1
	var-optionE3 = #ea2238
	var-optionBlackfriday = #000000
	var-optionCybermonday = #2c6f86
	var-optionJulkalender = #cc0000
	var-optionGreenfriday = #41a500
	var-optionMellandagsrea = #af0035
	var-optionBack2school = #4d9edd
	/* COLOR VARIABLES - MISC */
	var-bbMark = #8e78a2
	var-bbSpoiler = #3779a1
	var-postHeader = #3779a1
	/* CODE VARIABLES */
	if (var-ad-height == 'locked-banner-ad-height')
		#siteHeader .banner
			height 121px !important
			overflow hidden !important
	if (var-ad-banner-spacer == 'locked-ad-banner-spacer')
		#siteHeader .banner
			margin-bottom 10px
	if (var-navigation-style == 'custom-navigation')
		.header:not(.tbButton)
			background-color transparent !important
			color var-text-2 !important
			.mainMenu
				.menu li.menuItem
					&.isSelected
						background-color var-on-background-1 !important
						height 25px
						font-weight 400
						a.link
							color var-theme !important
							padding-top 7px
			.subMenu
				.menu li.menuItem
					&.isSelected
						background-color var-on-background-1 !important
						height 20px
						font-weight 400
						a.link
							color var-theme !important
							padding-top 5px
			.mainMenu,
			.subMenu
				.rborder
					border-bottom none !important
				.menu li
					border none !important
					border-radius var-border-radius-button
					box-shadow var-box-shadow !important
					&.menuItem
						background-color var-on-background-1 !important
						margin-right 5px
						margin-bottom 2px
						&.isSelected a
							color var-text-1 !important
							font-weight 400
						&:hover
							background-color var-highlight !important
							box-shadow var-box-shadow !important
							color var-text-1 !important
						a
							color var-text-2 !important
							&:hover
								color var-text-1 !important
			.main
				border-right none !important
				border-bottom none !important
				background-color var-background-1 !important
		#better-sweclockers-dark-theme-toggle
			margin-left 0 !important
		if (var-search-style == 'default-search')
			.ninjaSearch
				border-color var-border-light !important
				.searchField .fieldWrap
					.inner,
					input
						background-color var-on-background-1 !important
			.header:not(.tbButton) .main .profile
				border-color var-border-light !important
	if (var-search-style == 'custom-search')
		.ninjaSearch
			width 180px
			border-left none !important
			h5
				color var-text-2 !important
			.searchField
				.fieldWrap
					border none !important
					background none !important
					height 24px !important
					.inner
						background-color transparent !important
						padding 0
					input
						color var-text-2
						background-color var-on-background-1 !important
						border-radius var-border-radius !important
						box-shadow var-box-shadow !important
						padding 3px 6px
						&:hover,
						&:focus
							color var-text-1 !important
							background-color var-highlight !important
					.fluid
						.inner
							padding-right 0
					.fixed
						display none
						.icon
							margin-top 3px
				a[title*=Google]
					background-color var-on-background-1 !important
					color var-text-1 !important
					border var-border-style !important
					&:hover
						background-color var-highlight !important
		.header:not(.tbButton) .main .profile
			border-left none !important
		if (var-navigation-style == 'default-navigation')
			.ninjaSearch .searchField .fieldWrap input
				background-color var-on-background-2 !important
	if (var-scrollbar-style == 'custom-scrollbar')
		::-webkit-scrollbar
			width 17px
			height 12px
		::-webkit-scrollbar-corner
			background var-background-1
		::-webkit-scrollbar-thumb
			background var-theme
			border-left 4px solid var-background-2 !important
			border-right 4px solid var-background-2 !important
		::-webkit-scrollbar-track
			background var-background-1
	filter(n)
		filter n
		-moz-filter n
		-ms-filter n
		-o-filter n
		-webkit-filter n
	border-radius(n)
		border-radius n
		-moz-border-radius n
		-ms-border-radius n
		-o-border-radius n
		-webkit-border-radius n
	box-shadow(n)
		box-shadow n
		-moz-box-shadow n
		-ms-box-shadow n
		-o-box-shadow n
		-webkit-box-shadow n
	user-select(n)
		user-select n
		-moz-user-select n
		-ms-user-select n
		-o-user-select n
		-webkit-user-select n
	#__laika_cnt\.textarea\.0
		background-color var-background-1 !important
		border var-border-style !important
		color var-text-2 !important
		&:focus
			box-shadow 0 0 0 !important
			color var-text-1 !important
	#__laika_cnt\.select\.0,
	#__laika_cnt\.select\.1,
	#__laika_cnt\.select\.2,
	#__laika_cnt\.select\.3,
	#__laika_cnt\.select\.4
		background-color darken(var-background-1, 40%) !important
		color var-text-2
		border var-border-style !important
	#__laika_cnt\.textbox\.0,
	#__laika_cnt\.textbox\.1,
	#__laika_cnt\.textbox\.2,
	#__laika_cnt\.textbox\.3,
	#__laika_cnt\.textbox\.4
		background-color darken(var-background-1, 40%) !important
		color var-text-2
		border var-border-style !important
		&:focus
			box-shadow 0 0 0 !important
			color var-text-1 !important
	/* WIDGETS */
	#wdgtSideRecentThreads
		background-color var-background-2 !important
		box-shadow var-box-shadow !important
		border-radius var-border-radius !important
		.plHead
			h5
				color var-theme !important
				&:hover
					text-decoration var-hover-text !important
					opacity var-hover-opacity !important
		.plBody
			.plItem
				.plLabel
					.plIcon
						filter var-theme-filter !important
					.plLabelText
						color var-text-2 !important
					&:hover
						.plLabelIcon,
						.plLabelText
							text-decoration var-hover-text !important
							opacity var-hover-opacity !important
				.plComments
					.plLabelText
						color var-theme !important
			.button
				background-color var-on-background-2 !important
				color var-text-2 !important
				border-radius var-border-radius-button !important
				&:hover
					background-color var-highlight !important
					color var-theme !important
	#recent-classifieds,
	#recent-reviews
		background-color var-background-2 !important
		box-shadow var-box-shadow !important
		border-radius var-border-radius !important
		.widget-head
			.title
				color var-theme !important
				&:hover
					opacity var-hover-opacity !important
			.title-secondary
				color var-text-2 !important
				a
					color var-text-1 !important
					&:hover
						opacity var-hover-opacity !important
		.widget-body
			.button
				&.bg-beige
					background-color var-beige !important
				&.bg-blue
					background-color var-blue !important
				&.bg-green
					background-color var-green !important
				&.bg-pink
					background-color var-pink !important
			.list-item
				.list-link
					color var-text-1 !important
					&:hover
						opacity var-hover-opacity !important
					.icon
						filter var-theme-filter !important
					.button
						border none !important
					.text-bold.color-white
						color var-theme !important
				.list-comment-bubble
					&:hover
						opacity var-hover-opacity !important
					&.color-orange
						color var-theme !important
					.icon
						filter var-theme-filter !important
	#wdgtSideExternalFeeds
		.plHead
			h5
				color var-theme !important
			p
				color var-text-2 !important
		.plBody
			h6
				color var-text-1 !important
			.plItem
				.plLabel
					.plIcon
						filter var-theme-filter !important
					.plLabelText
						color var-text-2 !important
					.plLabelDate
						color var-theme !important
					&:hover
						.plLabelIcon,
						.plLabelText
							text-decoration var-hover-text !important
							opacity var-hover-opacity !important
	#wdgtMainRecentNews
		background-color var-background-2 !important
		box-shadow var-box-shadow !important
		border-radius var-border-radius !important
		.mwHead
			h5
				color var-theme !important
				&:hover
					text-decoration var-hover-text !important
					opacity var-hover-opacity !important
		.mwBody
			h6
				color var-text-1 !important
			.mwItem
				.mwLabel
					.mwIcon
						filter var-theme-filter !important
					.mwLabelDate
						color var-text-1 !important
					.mwLabelText
						color var-text-2 !important
					&:hover
						.mwLabelIcon,
						.mwLabelDate,
						.mwLabelText
							text-decoration var-hover-text !important
							opacity var-hover-opacity !important
				.mwComments
					.mwLabelText
						color var-theme !important
	#wdgtMainRecentThreads
		background-color var-background-2 !important
		box-shadow var-box-shadow !important
		border-radius var-border-radius !important
		.mwHead
			h5
				color var-theme !important
				&:hover
					text-decoration var-hover-text !important
					opacity var-hover-opacity !important
		.mwBody
			h6
				color var-text-1 !important
			.mwItem
				.mwLabel
					.mwIcon
						filter var-theme-filter !important
					.mwLabelText
						color var-text-2 !important
					&:hover
						.mwLabelIcon,
						.mwLabelText
							text-decoration var-hover-text !important
							opacity var-hover-opacity !important
	#wdgtSideRecentClassifieds
		& > div
			&:nth-child(2),
			&:nth-child(3),
			&:nth-child(4),
			&:nth-child(5)
				> h6
					color var-text-1 !important
	#prisjakt-popular
		background-color var-background-2 !important
		box-shadow var-box-shadow !important
		border-radius var-border-radius !important
		.widget-head
			.title span
				color var-theme !important
			.title-secondary a
				color var-text-2 !important
				&:hover
					opacity var-hover-opacity !important
		.widget-body
			.list-link
				.icon
					filter var-theme-filter !important
				.list-link-text
					.title
						color var-text-1 !important
					.price
						color var-theme !important
				.thumb
					background-color var-background-2 !important
					img
						opacity var-image-opacity !important
				&:hover
					opacity var-hover-opacity !important
	/* BETTER SWECLOCKERS */
	#better-sweclockers
		font-family Arial, Helvetica, Meiryo, sans-serif
		font-size 14px
		label
			color var-text-1
		.s5fieldset
			border-radius var-border-radius
			border var-border-style !important
			legend
				color var-theme !important
		textarea#better-sweclockers-preference-quote_signature_message,
		textarea#better-sweclockers-preference-custom_css_code,
		input#better-sweclockers-preference-textarea_size_small,
		input#better-sweclockers-preference-textarea_size_large,
		input#better-sweclockers-preference-dark_theme_time_on,
		input#better-sweclockers-preference-dark_theme_time_off,
		input#better-sweclockers-preference-dark_theme_interval
			background-color var-on-background-2 !important
			border var-border-style
			border-radius var-border-radius-button
		::-webkit-calendar-picker-indicator
			filter var-light-filter !important
		textarea#better-sweclockers-preference-quote_signature_message,
		textarea#better-sweclockers-preference-custom_css_code
			margin-top 8px
		.better-sweclockers-code-input::after
			color var-warning !important
			font-weight 500 !important
	#better-sweclockers-dark-theme-toggle
		background-image url('https://www.sweclockers.com/gfx/spritemap2x.png') !important
		background-position -466px -95px
		background-size 600px auto
		background-color var-on-background-1 !important
		opacity 1
		&.better-sweclockers-dark-theme-active
			background-color #696969 !important
		&:hover
			background-color var-highlight !important
	#better-sweclockers-document
		.better-sweclockers-editing-tools
			.button
				background-color transparent !important
				border none !important
				margin-right 4px !important
				&:hover
					background-color var-highlight !important
			.better-sweclockers-color-palette .button
				margin-right 4px !important
		.better-sweclockers-textarea-size
			.tbButton
				.btnIcon
					filter var-grey-filter !important
	/* REST OF CODE */
	#articleAuthors
		hr
			color var-border-light
	#contactForm
		button
			background var-background-1 !important
			background-color var-background-1 !important
			color var-text-2 !important
			&:hover
				background var-highlight !important
				background-color var-highlight !important
				color var-text-1 !important
		label
			color var-text-2 !important
	#content-header
		padding-top 20px
		h1
			color var-text-1
		p
			color var-text-2
	#form_inputs
		.policy_container .checkboxspan
			color var-text-2 !important
			a
				color var-theme !important
		a.selectBox
			text-shadow none !important
			color var-text-1 !important
		div.checkbox
			filter contrast(0.89) hue-rotate(-63deg) invert(1)
		label
			color var-text-1 !important
		textarea,
		input
			background-color var-background-1 !important
			color var-text-2 !important
	#revolut-ad
		background-color var-background-2 !important
		.button
			background var-background-1 !important
			border var-border-style !important
			color var-text-2 !important
			&:hover
				background-color var-highlight !important
		.text
			color var-text-2 !important
			> a
				color var-theme !important
		.title > a
			color var-text-1 !important
		> .shadow
			background -moz-linear-gradient(top, transparent 0, transparent 10%, var-background-1 100%) !important
			background -webkit-gradient(linear, left top, left bottom, color-stop(0, transparent), color-stop(45%, transparent), color-stop(100%, var-background-1)) !important
			background -webkit-linear-gradient(top, transparent 0, transparent 10%, var-background-1 100%) !important
			background -o-linear-gradient(top, transparent 0, transparent 10%, var-background-1 100%) !important
			background -ms-linear-gradient(top, transparent 0, transparent 10%, var-background-1 100%) !important
			background linear-gradient(to bottom, transparent 0, transparent 10%, var-background-1 100%) !important
	#single.content_holder
		background-color var-background-2 !important
	#subscribeForm > fieldset > button
		background var-background-1 !important
		background-color var-background-1 !important
		color var-text-2 !important
		&:hover
			background var-highlight !important
			background-color var-highlight !important
			color var-text-1 !important
	.activePolls a
		color var-theme !important
		&:hover
			text-decoration var-hover-text !important
			opacity var-hover-opacity !important
	.ad
		opacity var-image-opacity !important
		&.adPanorama,
		&.adModule,
		&.adInsider,
		&.adOutsider
			iframe
				border-radius var-border-radius
				background-color darken(var-background-1, 40%)
				box-shadow var-box-shadow
		&.adModule
			border-bottom none !important
		.adFullHeaderChild
			background-color var-background-1 !important
	.articleActions .action.proofArticle
		.actionIcon
			filter var-theme-filter !important
		.actionLabel
			color var-theme !important
			&:hover
				text-decoration var-hover-text !important
	.articleBBCode
		.bbAward
			.badge
				border-radius 50% !important
				box-shadow var-box-shadow !important
			.reason
				background-color var-on-background-2 !important
				border-radius var-border-radius !important
				box-shadow var-box-shadow !important
		.bbBlockQuote&:after
			color var-theme
		.bbExpander
			.expanderFooter
				color var-text-2 !important
				&:hover
					text-decoration var-hover-text !important
					opacity var-hover-opacity !important
			.expanderHeader
				color var-theme !important
				border-bottom var-border-style-light !important
				&:hover
					text-decoration var-hover-text !important
					opacity var-hover-opacity !important
			> .expanderFrame > .expanderFade
				background -moz-linear-gradient(top, transparent 0, transparent 10%, var-background-1 100%) !important
				background -webkit-gradient(linear, left top, left bottom, color-stop(0, transparent), color-stop(45%, transparent), color-stop(100%, var-background-1)) !important
				background -webkit-linear-gradient(top, transparent 0, transparent 10%, var-background-1 100%) !important
				background -o-linear-gradient(top, transparent 0, transparent 10%, var-background-1 100%) !important
				background -ms-linear-gradient(top, transparent 0, transparent 10%, var-background-1 100%) !important
				background linear-gradient(to bottom, transparent 0, transparent 10%, var-background-1 100%) !important
		.bbImage
			img
				opacity var-image-opacity !important
				border-radius var-border-radius !important
				box-shadow var-box-shadow !important
		.bbVideo
			iframe
				opacity var-image-opacity !important
				border-radius var-border-radius !important
				box-shadow var-box-shadow !important
		.bbPoll
			.optionBar > div
				background var-theme !important
				border none !important
			.pollOptions
				.votesAbs
					color var-text-2 !important
				.votesRel
					color var-text-2 !important
			.pollQuestion
				color var-text-1 !important
				border-bottom var-border-style-light !important
			.pollSums
				.pollMessage
					color var-text-2 !important
				.pollControls
					button
						color var-text-2 !important
						background-color var-on-background-2-button !important
						&:hover
							color var-theme !important
							background-color var-highlight !important
				.totalVotes
					color var-text-1 !important
					border-top var-border-style-light !important
			> .inner
				background-color var-on-background-2 !important
				border var-border-style !important
				border-radius var-border-radius !important
				color var-text-2 !important
		.bbPrisjakt a
			&.prices
				color var-text-2 !important
			&:hover.prices
				text-decoration var-hover-text !important
				opacity var-hover-opacity !important
		.bbRelatedBox
