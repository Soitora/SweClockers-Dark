		.authors-byline a
			color $text-1 !important
		.category
			color $text-2 !important
			&:hover
				text-decoration $hover-text !important
				opacity $hover-opacity !important
		.comments
			color $theme !important
			.icon
				filter $theme-filter !important
		.listItem a:hover .n
			text-decoration $hover-text !important
			opacity $hover-opacity !important
