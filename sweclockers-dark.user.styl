/* ==UserStyle==
@name			SweClockers Dark
@namespace		https://github.com/Soitora
@version		3.0.0-beta.6
@description	A modern dark theme for SweClockers
@author			Soitora <simon.mattila@protonmail.com> (https://soitora.com)
@homepageURL	https://github.com/Soitora/SweClockers-Dark
@supportURL		https://github.com/Soitora/SweClockers-Dark/issues
@updateURL		https://raw.githubusercontent.com/Soitora/SweClockers-Dark/master/sweclockers-dark.user.styl
@license		MPL-2.0
@preprocessor	stylus

@var	color	usercss-background-1 "Background color 1" #121212
@var	color	usercss-background-2 "Background color 2" #1D1D1D
@var	color	usercss-background-3 "Background color 3" #0B0B0D
@var	color	usercss-text-1 "Text color 1" #C8C8C8
@var	color	usercss-text-2 "Text color 2" #AAAAAA
@var	color	usercss-border "Border color" #08080B
@var	color	usercss-border-menus "Border menu color" #000000
@var	color	usercss-highlight "Highlight color" #232326
@var	color	usercss-warning "Warning color" #EE4757
@var	text	usercss-warning-filter "Warning filter" brightness(0) saturate(100%) invert(43%) sepia(88%) saturate(1483%) hue-rotate(326deg) brightness(89%) contrast(113%)
@var	color	usercss-theme "Theme color" #F19A57
@var	text	usercss-theme-filter "Theme filter" brightness(0) saturate(100%) invert(82%) sepia(26%) saturate(3168%) hue-rotate(320deg) brightness(99%) contrast(91%)

==/UserStyle== */
@-moz-document domain("sweclockers.com")
	/* User variables */
	$background-1 = usercss-background-1
	$background-2 = usercss-background-2
	$background-3 = usercss-background-3
	$text-1 = usercss-text-1
	$text-2 = usercss-text-2
	$border = usercss-border
	$border-menus = usercss-border-menus
	$menu-buttons = #2E2E2E
	$highlight = usercss-highlight
	$warning = usercss-warning
	$warning-filter = usercss-warning-filter
	$theme = usercss-theme
	$theme-filter = usercss-theme-filter
	/* Global variables */
	$border-light = darken($text-2, 65%)
	$border-style = 1px solid $border
	$border-style-light = 1px solid $border-light
	$border-radius = 0.6rem
	$hover-opacity = 0.8
	$hover-text = none
	$box-shadow = 0 0 0 1px $border
	$box-shadow-inset = inset 0 0 0 1px $border
	$text-shadow = 1px 1px 2px $black
	$grey-filter = sepia(0) saturate(0.29) brightness(67%) contrast(184%) hue-rotate(179deg) invert(8%)
	$light-filter = brightness(0) saturate(100%) invert(92%) sepia(0) saturate(489%) hue-rotate(229deg) brightness(90%) contrast(86%)
	$blue-filter = brightness(0) saturate(100%) invert(41%) sepia(50%) saturate(563%) hue-rotate(159deg) brightness(94%) contrast(89%)
	/* Colors variables */
	$black = #000000
	$white = #ffffff
	$beige = #9c6500
	$blue = #305d84
	$green = #1c5a00
	$pink = #b34d84
	/* Misc color variables */
	$c-bbMark = #8e78a2
	$c-optionCes = #639b48
	$c-optionCes-border = #51743e
	$c-optionCes-text = #222222
	$c-optionJulkalender = #cc0000
	$c-optionJulkalender-text = #cccccc
	$c-optionMellandagsrea = #ac0c38
	$c-optionMellandagsrea-border = #780024
	$c-optionMellandagsrea-text = #cccccc
	$c-optionBlackFriday = #000000
	$c-optionBack2school = #4d9edd
	$c-optionBack2school-text = #222222
	$c-optionComputex = #e4007f
	$c-optionComputex-border = #c6006e
	$c-optionComputex-text = #cccccc
	$c-optionCybermonday = #49b2d6
	$c-optionCybermonday-border = #419fbf
	$c-optionCybermonday-text = #222222
	$c-optionDreamhack = #5e2f8e
	$c-optionDreamhack-border = #402061
	$c-optionDreamhack-text = #cccccc
	$c-optionE3 = #ea2238
	$c-optionE3-border = #aa1929
	$c-optionE3-text = #cccccc
	$c-optionGreenfriday = #41a500
	$c-optionGreenfriday-border = #2b760a
	$c-optionGreenfriday-text = #222222
	$c-optionPaskrea = #ffd200
	$c-optionPaskrea-border = #d9a92b
	$c-optionPaskrea-text = #222222
	$c-optionVr = #8111e1
	$c-optionVr-border = #6a0eb9
	$c-optionVr-text = #cccccc
	$c-bbSpoiler = #3779a1
	$c-postHeader = #3779a1
	/* Mixins */
	filter(n)
		filter n
		-moz-filter n
		-ms-filter n
		-o-filter n
		-webkit-filter n
	border-radius(n)
		border-radius n
		-moz-border-radius n
		-ms-border-radius n
		-o-border-radius n
		-webkit-border-radius n
	box-shadow(n)
		box-shadow n
		-moz-box-shadow n
		-ms-box-shadow n
		-o-box-shadow n
		-webkit-box-shadow n
	user-select(n)
		user-select n
		-moz-user-select n
		-ms-user-select n
		-o-user-select n
		-webkit-user-select n
	/* Main Code */
	#__laika_cnt\.textarea\.0
		background-color $background-1 !important
		border $border-style !important
		color $text-2 !important
		&:focus
			box-shadow 0 0 0 !important
			color $text-1 !important
	#__laika_cnt\.select\.0,
	#__laika_cnt\.select\.1,
	#__laika_cnt\.select\.2,
	#__laika_cnt\.select\.3,
	#__laika_cnt\.select\.4
		background-color $background-3 !important
		color $text-2
		border $border-style !important
	#__laika_cnt\.textbox\.0,
	#__laika_cnt\.textbox\.1,
	#__laika_cnt\.textbox\.2,
	#__laika_cnt\.textbox\.3,
	#__laika_cnt\.textbox\.4
