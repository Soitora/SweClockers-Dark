/* ==UserStyle==
@name			SweClockers Dark
@namespace		https://github.com/Soitora
@version		3.0.0-beta.4
@description	A modern dark theme for SweClockers
@author			Soitora <simon.mattila@protonmail.com> (https://soitora.com)
@homepageURL	https://github.com/Soitora/SweClockers-Dark
@supportURL		https://github.com/Soitora/SweClockers-Dark/issues
@updateURL		https://raw.githubusercontent.com/Soitora/SweClockers-Dark/master/sweclockers-dark.user.styl
@license		MPL-2.0
@preprocessor	stylus

@var	color	usercss-background-1 "Background color 1" #1A1A1D
@var	color	usercss-background-2 "Background color 2" #1E1E21
@var	color	usercss-background-3 "Background color 3" #18181B
@var	color	usercss-background-4 "Background color 4" #121215
@var	color	usercss-text-1 "Text color 1" #C8C8C8
@var	color	usercss-text-2 "Text color 2" #AAAAAA
@var	color	usercss-border "Border color" #17171A
@var	color	usercss-highlight "Highlight color" #232326
@var	color	usercss-warning "Warning color" #EF4A4B
@var	color	usercss-theme "Theme color" #F19A57
@var	text	usercss-theme-filter "Theme filter" brightness(0) saturate(100%) invert(82%) sepia(26%) saturate(3168%) hue-rotate(320deg) brightness(99%) contrast(91%)

==/UserStyle== */
@-moz-document domain("sweclockers.com")
	/* User variables */
	$background-1 = usercss-background-1
	$background-2 = usercss-background-2
	$background-3 = usercss-background-3
	$background-4 = usercss-background-4
	$text-1 = usercss-text-1
	$text-2 = usercss-text-2
	$border = usercss-border
	$highlight = usercss-highlight
	$warning = usercss-warning
	$theme = usercss-theme
	$theme-filter = usercss-theme-filter
	/* Global variables */
	$border-style = 1px solid $border
	$border-radius = 0.6rem
	$hover-opacity = 0.75
	$hover-text = none
	$box-shadow = 0 0 0 1px $border
	$text-shadow = 1px 1px 2px #000000
	$grey-filter = sepia(0) saturate(0.29) brightness(67%) contrast(184%) hue-rotate(179deg) invert(8%)
	/* Misc colors */
	$black = #000000
	$white = #ffffff
	$color-bg-beige = #9c6500
	$color-bg-blue = #305d84
	$color-bg-green = #1c5a00
	$color-bg-pink = #b34d84
	$color-bbMark = #8e78a2
	$color-optionCes = #639b48
	$color-optionCes-border = #51743e
	$color-optionCes-text = #222222
	$color-optionJulkalender = #cc0000
	$color-optionJulkalender-text = #cccccc
	$color-optionMellandagsrea = #ac0c38
	$color-optionMellandagsrea-border = #780024
	$color-optionMellandagsrea-text = #cccccc
	$color-optionBlackFriday = #000000
	$color-optionBack2school = #4d9edd
	$color-optionBack2school-text = #222222
	$color-optionComputex = #e4007f
	$color-optionComputex-border = #c6006e
	$color-optionComputex-text = #cccccc
	$color-optionCybermonday = #49b2d6
	$color-optionCybermonday-border = #419fbf
	$color-optionCybermonday-text = #222222
	$color-optionDreamhack = #5e2f8e
	$color-optionDreamhack-border = #402061
	$color-optionDreamhack-text = #cccccc
	$color-optionE3 = #ea2238
	$color-optionE3-border = #aa1929
	$color-optionE3-text = #cccccc
	$color-optionGreenfriday = #41a500
	$color-optionGreenfriday-border = #2b760a
	$color-optionGreenfriday-text = #222222
	$color-optionPaskrea = #ffd200
	$color-optionPaskrea-border = #d9a92b
	$color-optionPaskrea-text = #222222
	$color-optionVr = #8111e1
	$color-optionVr-border = #6a0eb9
	$color-optionVr-text = #cccccc
	$color-bbSpoiler = #3779a1
	$color-postHeader = #3779a1
	/* Mixins */
	filter(n)
		filter n
		-moz-filter n
		-ms-filter n
		-o-filter n
		-webkit-filter n
	border-radius(n)
		border-radius n
		-moz-border-radius n
		-ms-border-radius n
		-o-border-radius n
		-webkit-border-radius n
	box-shadow(n)
		box-shadow n
		-moz-box-shadow n
		-ms-box-shadow n
		-o-box-shadow n
		-webkit-box-shadow n
	user-select(n)
		user-select n
		-moz-user-select n
		-ms-user-select n
		-o-user-select n
		-webkit-user-select n
